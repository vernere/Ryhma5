  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "cypress:coverage": "nyc cypress run",
    "test:coverage": "bun test --coverage",
    "test:e2e": "cypress run",
    "test:e2e:coverage": "cypress run && nyc --all report --report-dir=coverage/e2e --reporter=json",
    "coverage:merge": "node scripts/merge-coverage.cjs",
    "coverage:report": "nyc report --reporter=html --reporter=text --temp-dir=coverage/merged",
    "coverage:full": "pwsh -c 'if (Test-Path .nyc_output) { Remove-Item .nyc_output/* -Force } ; Get-ChildItem -Path coverage -Recurse -File | Remove-Item -Force' && bun run test:coverage && bun run test:e2e:coverage && bun run coverage:merge && bun run coverage:report"
  },
